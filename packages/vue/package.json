{
  "name": "@danegg-ui/icons-vue",
  "version": "1.0.1",
  "description": "Vue components of Danegg UI Icons collection.",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "keywords": [],
  "author": "feng",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "sideEffects": false,
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^6.0.0",
    "@pnpm/find-workspace-packages": "^6.0.6",
    "@pnpm/logger": "^5.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "esbuild": "^0.17.18",
    "esbuild-plugin-globals": "^0.2.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5",
    "unplugin-vue": "^4.1.0",
    "vite": "^4.3.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.4"
  }
}
